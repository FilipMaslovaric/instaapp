<p id="notice"><%= notice %></p>

<h1>Photos</h1>

    <% @photos.each do |photo| %>
        <p><%= image_tag photo.image_url %></p>
        <p><%= photo.user.email %></p>
        <p><%= link_to 'Show', photo %></p>
        <p><%= link_to 'Edit', edit_photo_path(photo) %></p>
        <p><%= link_to 'Destroy', photo, method: :delete, data: { confirm: 'Are you sure?' } %></p>
    <% end %>


<br>

<%= link_to 'New Photo', new_photo_path %>
